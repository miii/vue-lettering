  "main": "dist/vue-lettering.common.js",
  "module": "dist/vue-lettering.esm.js",
  "unpkg": "dist/vue-lettering.umd.min.js",
  "jsdelivr": "dist/vue-lettering.umd.min.js",
  "types": "types/index.d.ts",
  "sideeffects": false,
  "files": [
    "dist/vue-lettering.common.js",
    "dist/vue-lettering.umd.min.js",
    "dist/vue-lettering.umd.js",
    "dist/vue-lettering.esm.js",
    "src",
    "types/index.d.ts"
  ],
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-plugin-typescript": "^4.2.0",
    "@vue/cli-service": "^4.2.0",
    "@vue/eslint-config-standard": "^5.1.0",
    "@vue/eslint-config-typescript": "^5.0.1",
    "@vuepress/plugin-google-analytics": "^1.3.1",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.1.2",
    "husky": "^4.2.3",
    "typescript": "^3.8.3",
    "vue": "^2.6.11",
    "vue-cli-plugin-p11n": "^0.4.0",
    "vue-template-compiler": "^2.6.11"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
